<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>rd.ebudget.servlet.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetRdGoal</servlet-name>
        <servlet-class>rd.ebudget.servlet.LookUp.GetRdGoal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetRDTactics</servlet-name>
        <servlet-class>rd.ebudget.servlet.LookUp.GetRDTactics</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetPersonInfo</servlet-name>
        <servlet-class>rd.ebudget.servlet.LookUp.GetPersonInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GovPolicyServlet</servlet-name>
        <servlet-class>rd.ebudget.servlet.GovPolicyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MofStrategicServlet</servlet-name>
        <servlet-class>rd.ebudget.servlet.MofStrategicServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetRdGoal</servlet-name>
        <url-pattern>/GetRdGoal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetRDTactics</servlet-name>
        <url-pattern>/GetRDTactics</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetPersonInfo</servlet-name>
        <url-pattern>/GetPersonInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GovPolicyServlet</servlet-name>
        <url-pattern>/GovPolicyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MofStrategicServlet</servlet-name>
        <url-pattern>/MofStrategicServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
