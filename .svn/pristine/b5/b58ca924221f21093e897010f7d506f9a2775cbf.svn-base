package rd.ebudget.object.projectManagement;

/**
 *
 * @author Mr.Tue
 */
public class ProjectActivity {
    private String project_id;
    private String project_activity_id;
    private String project_activity_name;
    private String project_activity_budget;


    public String getProject_id() {
        return project_id;
    }

    /**
     * @param project_id the project_id to set
     */
    public void setProject_id(String project_id) {
        this.project_id = project_id;
    }

    /**
     * @return the project_activity_id
     */
    public String getProject_activity_id() {
        return project_activity_id;
    }

    /**
     * @param project_activity_id the project_activity_id to set
     */
    public void setProject_activity_id(String project_activity_id) {
        this.project_activity_id = project_activity_id;
    }

    /**
     * @return the project_activity_name
     */
    public String getProject_activity_name() {
        return project_activity_name;
    }

    /**
     * @param project_activity_name the project_activity_name to set
     */
    public void setProject_activity_name(String project_activity_name) {
        this.project_activity_name = project_activity_name;
    }

    /**
     * @return the project_activity_budget
     */
    public String getProject_activity_budget() {
        return project_activity_budget;
    }

    /**
     * @param project_activity_budget the project_activity_budget to set
     */
    public void setProject_activity_budget(String project_activity_budget) {
        this.project_activity_budget = project_activity_budget;
    }
    

}
